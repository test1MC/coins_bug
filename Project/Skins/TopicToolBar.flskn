<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="2"
  SkinType="Component"
  Comment="HTML5 Toolbar skin"
  ComponentType="TopicToolbar"
  EnableResponsiveOutput="True">
  <Stylesheet>
    <Styles>
      <Style
        Name="Toolbar">
        <Properties>
          <Property
            Name="MarginRight">0px</Property>
          <Property
            Name="MarginTop">20px</Property>
          <Property
            Name="MarginBottom">5px</Property>
          <Property
            Name="Float">right</Property>
        </Properties>
      </Style>
      <Style
        Name="ToolbarItem">
        <Classes>
          <StyleClass
            Name="Comments"
            Custom="true">
            <Properties>
              <Property
                Name="BackgroundRepeat">no-repeat</Property>
              <Property
                Name="BackgroundPosition">center center</Property>
              <Property
                Name="Click">document.location.href="mailto:coinslms@coins-global.com?subject=Feedback (Topic: "+document.title+")&amp;body=File: "+location.href+"%0A%0AComments:%0A%0A";</Property>
              <Property
                Name="BackgroundColor">#ff8c00</Property>
              <Property
                Name="BackgroundImage">url('mail.gif')</Property>
            </Properties>
          </StyleClass>
        </Classes>
      </Style>
      <Medium
        Name="Tablet">
        <Style
          Name="ToolbarItem">
          <Classes>
            <StyleClass
              Name="PrintTopic">
              <Properties>
                <Property
                  Name="BackgroundRepeat">no-repeat</Property>
                <Property
                  Name="BackgroundPosition">center center</Property>
              </Properties>
            </StyleClass>
            <StyleClass
              Name="Comments">
              <Properties>
                <Property
                  Name="BackgroundRepeat">no-repeat</Property>
                <Property
                  Name="BackgroundPosition">center center</Property>
              </Properties>
            </StyleClass>
          </Classes>
        </Style>
      </Medium>
      <Medium
        Name="Mobile">
        <Style
          Name="Toolbar">
          <Properties>
            <Property
              Name="Display">none</Property>
          </Properties>
        </Style>
        <Style
          Name="ToolbarItem">
          <Classes>
            <StyleClass
              Name="PrintTopic">
              <Properties>
                <Property
                  Name="BackgroundRepeat">no-repeat</Property>
                <Property
                  Name="BackgroundPosition">center center</Property>
              </Properties>
            </StyleClass>
            <StyleClass
              Name="Comments">
              <Properties>
                <Property
                  Name="BackgroundRepeat">no-repeat</Property>
                <Property
                  Name="BackgroundPosition">center center</Property>
              </Properties>
            </StyleClass>
          </Classes>
        </Style>
      </Medium>
    </Styles>
    <Stylesheet></Stylesheet>
    <Resources>
      <Resource
        Name="mail.gif"></Resource>
    </Resources>
  </Stylesheet>
  <TopicToolbar
    EnableCustomLayout="true"
    Buttons="SelectSkin|TopicRatings|Comments|Print|Separator">
    <Script></Script>
  </TopicToolbar>
  <Strings>
    <Stylesheet
      lang="en-gb">
      <Stylesheet>
        <Styles>
          <Style
            Name="Toolbar">
            <Properties>
              <Property
                Name="MarginBottom">33px</Property>
              <Property
                Name="MarginRight">-4px</Property>
              <Property
                Name="Float">right</Property>
            </Properties>
          </Style>
          <Medium
            Name="Mobile">
            <Style
              Name="Toolbar">
              <Properties>
                <Property
                  Name="Display">none</Property>
              </Properties>
            </Style>
          </Medium>
          <Style
            Name="ToolbarItem">
            <Classes>
              <StyleClass
                Name="Comments">
                <Properties>
                  <Property
                    Name="Tooltip">Send feedback on this topic</Property>
                </Properties>
              </StyleClass>
            </Classes>
          </Style>
        </Styles>
      </Stylesheet>
    </Stylesheet>
  </Strings>
</CatapultSkin>